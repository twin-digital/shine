{
  "name": "@twindigital/shine",
  "version": "0.0.0",
  "description": "GitHub app that simplifies sharing configurations across repositories.",
  "license": "MIT",
  "engines": {
    "node": ">=12",
    "yarn": ">=2"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "lint": "run-p 'lint:*'",
    "lint:packageJson": "npmPkgJsonLint --ignorePath .gitignore .",
    "lint:src": "eslint --cache --report-unused-disable-directives --ext '.ts,.js,.tsx,.jsx,.json' .",
    "watch": "tsc --watch"
  },
  "devDependencies": {
    "@twindigital/eslint-config": "1.0.1",
    "@types/lodash": "^4.14.169",
    "@types/node-fetch": "^2.5.10",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-json": "^3.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-package-json-lint": "^5.1.0",
    "npm-run-all": "4.1.5",
    "typescript": "*"
  },
  "author": "Sean Kleinjung <sean@twindigital.io>",
  "bugs": {
    "url": "https://github.com/twin-digital/shine/issues"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://github.com/twin-digital/shine",
  "keywords": [
    "shine",
    "configuration"
  ],
  "publishConfig": {
    "access": "public"
  }
}
